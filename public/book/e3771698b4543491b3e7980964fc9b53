                                </else>
                            </if>
                        </else>
                    </if>
                </then>
                <else>
                    <if expression="hasil=2">
                        <then>
                            <output expression="&quot;mengambil uang atm (Y atau N )&quot;" newline="True"/>
                            <input variable="pilihan"/>
                            <if expression="pilihan=&quot;Y&quot; or pilihan=&quot;y&quot;">
                                <then>
                                    <output expression="&quot;Selamat uang anda telah keluar&quot;" newline="True"/>
                                </then>
                                <else>
                                    <if expression="pilihan=&quot;N&quot; or pilihan=&quot;n&quot;">
                                        <then>
                                            <output expression="&quot;perintah yang anda masukkan yes&quot;" newline="True"/>
                                        </then>
                                        <else>
                                            <output expression="&quot;Masukan jawaban yang benar&quot;" newline="True"/>
                                        </else>
                                    </if>
                                </else>
                            </if>
                        </then>
                        <else>
                            <if expression="hasil=3">
                                <then>
                                    <output expression="&quot;Apakah ingin mengganti ban bocor ( Y / N )&quot;" newline="True"/>
                                    <input variable="pilihan"/>
                                    <if expression="pilihan=&quot;Y&quot; or pilihan=&quot;y&quot;">
                                        <then>
                                            <output expression="&quot;Selamat ban anda sudah normal selamat berkendara&quot;" newline="True"/>
                                        </then>
                                        <else>
                                            <if expression="pilihan=&quot;N&quot; or pilihan=&quot;n&quot;">
                                                <then>
                                                    <output expression="&quot;perintah yang anda masukkan yes&quot;" newline="True"/>
                                                </then>
                                                <else>
                                                    <output expression="&quot;Masukan jawaban yang benar&quot;" newline="True"/>
                                                </else>
                                            </if>
                                        </else>
                                    </if>
                                </then>
                                <else>
                                    <output expression="&quot;perintah yang anda masukkan salah&quot;" newline="True"/>
                                </else>
                            </if>
                        </else>
                    </if>
                </else>
            </if>
        </body>
