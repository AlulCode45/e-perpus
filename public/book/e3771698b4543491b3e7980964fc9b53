            <declare name="hasil" type="Integer" array="False" size=""/>
            <declare name="pilihan" type="String" array="False" size=""/>
            <output expression="&quot;pilih pilihan &quot;" newline="True"/>
            <output expression="&quot;1. menjadi mahasiswa unitomo &quot;" newline="True"/>
            <output expression="&quot;2.mengambil uang atm (anjungan tunai madiri)&quot;" newline="True"/>
            <output expression="&quot; 3. mengganti ban bocor &quot;" newline="False"/>
            <input variable="hasil"/>
            <if expression="hasil=1">
                <then>
                    <output expression="&quot; apakah ingin menjadi mahasiswa unitomo ?(Y atau N )&quot;" newline="True"/>
                    <input variable="pilihan"/>
                    <if expression="pilihan=&quot;Y&quot; or pilihan=&quot;y&quot;">
